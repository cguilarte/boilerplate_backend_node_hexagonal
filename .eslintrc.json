{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        /* "quotes":"off",
        "semi":"off",
        "comma-dangle":"off",
        "space-before-function-paren":"off" */
    }
}
